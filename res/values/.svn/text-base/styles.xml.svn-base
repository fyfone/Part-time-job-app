<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>

    <style name="main_bottom_bar_linear">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">@dimen/bottom_bar_height</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center</item>
        <item name="android:orientation">vertical</item>
    </style>

    <!-- 界面标题头 -->
    <style name="title_bar_font">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:textColor">#f0f0f0</item>
        <item name="android:textSize">25sp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="main_bottom_bar_image">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="main_bottom_bar_text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingTop">5dp</item>
        <item name="android:textColor">#707070</item>
    </style>

    <style name="divider">
        <item name="android:divider">@color/line</item>
        <item name="android:headerDividersEnabled">false</item>
        <item name="android:dividerHeight">@dimen/line</item>
    </style>

    <!-- 职位列表item -->
    <style name="item_title">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginRight">@dimen/item_title_right</item>
        <item name="android:textSize">@dimen/text_size</item>
        <item name="android:singleLine">true</item>
        <item name="android:textColor">@color/text_333333</item>
    </style>

    <style name="item_date">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/text_size</item>
        <item name="android:singleLine">true</item>
        <item name="android:textColor">@color/text_888</item>
    </style>

    <!-- 申请记录item -->
    <style name="sqjl_item_sqsj">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginRight">@dimen/item_title_right</item>
        <item name="android:textSize">@dimen/text_size</item>
        <item name="android:singleLine">true</item>
        <item name="android:textColor">@color/text_333333</item>
    </style>

    <style name="sqjl_item_date">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textSize">@dimen/text_size</item>
        <item name="android:singleLine">true</item>
        <item name="android:textColor">@color/text_333333</item>
    </style>

    <style name="item_cn">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:singleLine">true</item>
        <item name="android:textSize">@dimen/text_size</item>
        <item name="android:textColor">@color/text_888</item>
    </style>

    <!-- 登录框LinearLayout -->
    <style name="title_login_box">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/login_edittext_height</item>
        <item name="android:layout_marginLeft">@dimen/button_left_right_margin</item>
        <item name="android:layout_marginRight">@dimen/button_left_right_margin</item>
        <item name="android:background">@drawable/button_fillet_blue</item>
        <item name="android:orientation">horizontal</item>
    </style>
    <!-- 登录框EditText -->
    <style name="login_box_et">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@null</item>
        <item name="android:textSize">@dimen/login_edittext_size</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="login_phone_et">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@null</item>
        <item name="android:textSize">@dimen/login_edittext_size</item>
        <item name="android:maxLength">@string/phone_length</item>
        <item name="android:inputType">phone</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="login_pwd_et">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@null</item>
        <item name="android:textSize">@dimen/login_edittext_size</item>
        <item name="android:singleLine">true</item>
        <item name="android:inputType">textPassword</item>
    </style>

    <!-- 竖线分割线 -->
    <style name="vertical_line">
        <item name="android:layout_width">1dp</item>
        <item name="android:layout_height">26dp</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:background">@color/vertical_line</item>
    </style>

    <!-- 按钮 -->
    <style name="button_sty">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/button_height</item>
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:layout_marginLeft">@dimen/button_left_right_margin</item>
        <item name="android:layout_marginRight">@dimen/button_left_right_margin</item>
        <item name="android:background">@drawable/button_login_bule</item>
        <item name="android:textColor">@color/background_white</item>
    </style>

    <!-- 分割线 -->
    <style name="line_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/line</item>
        <item name="android:background">@color/line</item>
    </style>

    <!-- 支付宝 -->
    <style name="zfb_linearlayout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:layout_marginTop">@dimen/card_left_right_padding</item>
        <item name="android:layout_marginBottom">@dimen/card_left_right_padding</item>
    </style>

    <style name="zfb_textview">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_marginLeft">@dimen/icon_right</item>
        <item name="android:layout_marginRight">@dimen/icon_right</item>
        <item name="android:textSize">@dimen/text_size</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="zfb_edittext">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1.3</item>
        <item name="android:singleLine">true</item>
        <item name="android:gravity">right</item>
        <item name="android:background">@color/edit</item>
        <item name="android:paddingTop">@dimen/card_left_right_padding</item>
        <item name="android:paddingBottom">@dimen/card_left_right_padding</item>
        <item name="android:paddingLeft">@dimen/line</item>
        <item name="android:paddingRight">@dimen/line</item>
    </style>

    <!-- 职位详情 -->
    <style name="salary">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/text_red</item>
        <item name="android:textSize">@dimen/text_size</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="jd_Linearlayout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">@dimen/item_bottom</item>
        <item name="android:layout_marginLeft">@dimen/item_left_right_margin</item>
        <item name="android:layout_marginRight">@dimen/item_left_right_margin</item>
    </style>

    <style name="title">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginRight">@dimen/item_title_bott</item>
        <item name="android:textSize">@dimen/text_size</item>
        <item name="android:singleLine">true</item>
        <item name="android:textColor">@color/text_333333</item>
    </style>

    <!-- 申请记录 -->
    <style name="sqjl_listview">
        <item name="android:layout_margin">@dimen/card_left_right_margin</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:cacheColorHint">#00000000</item>
        <item name="android:dividerHeight">0dp</item>
        <item name="android:fadingEdge">none</item>
    </style>

    <style name="alert_line" >
         <item name="android:layout_width">match_parent</item>
  	    <item name="android:layout_height">1dp</item>
  	    <item name="android:background">@color/dialog_line</item>
    </style>
    
    <style name="alert_line2" >
         <item name="android:layout_width">match_parent</item>
  	    <item name="android:layout_height">1dp</item>
    </style>
</resources>